using System;

class Program
{
    // Procedimento para preencher um vetor com as notas de 10 alunos
    static void PreencherNotas(double[] notas)
    {
        for (int i = 0; i < notas.Length; i++)
        {
            Console.Write("Digite a nota do aluno {0}: ", i + 1);
            notas[i] = double.Parse(Console.ReadLine());
        }
    }

    // Procedimento para calcular a média e contar quantos alunos obtiveram nota acima da média
    static void CalcularMediaEContar(double[] notas)
    {
        double soma = 0;
        foreach (double nota in notas)
        {
            soma += nota;
        }

        double media = soma / notas.Length;
        int contagemAcimaDaMedia = 0;

        foreach (double nota in notas)
        {
            if (nota > media)
            {
                contagemAcimaDaMedia = contagemAcimaDaMedia + 1;
            }
        }

        Console.WriteLine("A média da turma é: {0}", media);
        Console.WriteLine("{0} alunos obtiveram nota acima da média.", contagemAcimaDaMedia);
    }

    static void Main()
    {
        double[] notas = new double[10];

        // Preenche o vetor com as notas dos alunos
        PreencherNotas(notas);

        // Calcula a média e conta quantos alunos obtiveram nota acima da média
        CalcularMediaEContar(notas);
    }
}
